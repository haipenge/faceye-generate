<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:generate 业务对像:Validate 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Validate 管理 首页(列表页) -->
	<definition name="generate.validate.manager" extends="default.manager">
	    <put-attribute name="default-center" value="generate.validate.center"></put-attribute>
	</definition>
	<definition name="generate.validate.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/generate/validate/validate_center.jsp"></put-attribute>
	</definition>
	
	<!-- Validate 新增,编辑页面 -->
	<definition name="generate.validate.update" extends="default.manager">
	    <put-attribute name="default-center" value="generate.validate.update.center"></put-attribute>
	</definition>
	<definition name="generate.validate.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/generate/validate/validate_update.jsp"></put-attribute>
	</definition>
	
	<!-- Validate 明细页 -->
	<definition name="generate.validate.detail" extends="default.manager">
	    <put-attribute name="default-center" value="generate.validate.detail.center"></put-attribute>
	</definition>
	<definition name="generate.validate.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/generate/validate/validate_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
